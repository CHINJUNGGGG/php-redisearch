{
    "name": "macfja/redisearch",
    "type": "library",
    "description": "PHP Client for RediSearch",
    "keywords": [
        "Redis",
        "RediSearch",
        "Search Engine",
        "Suggestion"
    ],
    "readme": "README.md",
    "license": "MIT",
    "authors": [
        {
            "name": "MacFJA"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-intl": "*",
        "composer/semver": "^3.2",
        "predis/predis": "^1.1",
        "respect/validation": "^2.0"
    },
    "require-dev": {
        "enlightn/security-checker": "^1.9",
        "ergebnis/composer-normalize": "^2.13",
        "friendsofphp/php-cs-fixer": "^3.0",
        "insolita/unused-scanner": "^2.3",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "phpmd/phpmd": "^2.10",
        "phpstan/phpstan": "^0.12.92",
        "phpunit/phpunit": "^8.5",
        "roave/security-advisories": "dev-latest",
        "rskuipers/php-assumptions": "^0.8.0",
        "sebastian/phpcpd": "^4.1",
        "vimeo/psalm": "^4.7"
    },
    "config": {
        "platform": {
            "php": "7.2"
        }
    },
    "autoload": {
        "psr-4": {
            "MacFJA\\RediSearch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MacFJA\\RediSearch\\tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/MacFJA/php-redisearch/issues",
        "source": "https://github.com/MacFJA/php-redisearch"
    }
}
